{
    "name": "thruster/symfony-json-body-parser-bundle",
    "type": "library",
    "description": "Thruster SymfonyJsonBodyParserBundle Bundle",
    "keywords": ["symfony-json-body-parser-bundle", "thruster", "json", "symfony", "request", "parser", "bundle"],
    "homepage": "https://thruster.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~2.3|~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1"
    },
    "autoload": {
        "psr-4": { "Thruster\\Bundle\\SymfonyJsonBodyParserBundle\\": "src" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
